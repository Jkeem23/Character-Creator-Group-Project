<h1>Created Characters</h1>
 
 <body>
   <div id = inner>
<table>
  <tr>
    <th>Character Name</th>
  </tr>
 
  <% @characters.each do |character| %>
    <tr>
      <td><%= character.name %></td>
      <td><%= link_to 'Show', character_path(character) %></td>
    </tr>
  <% end %>
</table>
</div>
</body>